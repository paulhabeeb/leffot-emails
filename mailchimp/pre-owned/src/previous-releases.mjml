<mj-section mj-class="no-padding">
    <mj-column>
        <mj-spacer height="3px" />
    </mj-column>
</mj-section>

<mj-section mj-class="shoe-group-header">
    <mj-column>
        <mj-text mj-class="shoe-group-title">
            <span class="thick-border">You May Have Missed</span>
        </mj-text>
    </mj-column>
</mj-section>

<mj-section mj-class="shoe-wrapper" padding="0 30px 15px">
